<h2>Homepage, user: <%= current_user.user_name %><br/>
<%= link_to 'logout', destroy_user_session_path, method: :delete %><br/>
<%= link_to 'Edit', edit_user_registration_path %></h2>

<% @items.each do |item| %>
<%= item.tumbnail_img %><br/>
<%= item.name %><br/>
<%= item.price %> $<br/>
<%= item.description %><br/>
<%= item.gender %><br/>
<%= item.item_likes.count %> likes


  <%= link_to 'like', item_item_likes_path(item), method: :post %>
  <%= link_to 'unlike', item_item_like_path(item, item.item_likes), method: :delete %>


<% end %>









